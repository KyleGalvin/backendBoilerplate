{
  "name": "backend_boilerplate",
  "version": "0.0.0",
  "description": "basic blank project setup. Hopefully saves you time configuring",
  "main": "index.js",
  "scripts": {
    "test": "mocha-typescript-watch -p tsconfig.json -o test/mocha.opts",
    "lint": "tslint --force 'src/**/*.ts' > ./tslint.err",
    "bundle": "mkdirp ./dist && tsc",
    "clean": "rm -rf ./dist",
    "serve": "npm run clean && npm run lint && npm run bundle && cat ./tslint.err && node ./dist/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyleGalvin/backendBoilerplate.git"
  },
  "author": "Kyle Galvin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KyleGalvin/backendBoilerplate/issues"
  },
  "homepage": "https://github.com/KyleGalvin/backendBoilerplate#readme",
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.5",
    "@types/bunyan": "^1.8.3",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.37",
    "mkdirp": "^0.5.1",
    "mocha": "^4.0.1",
    "mocha-typescript": "^1.1.12",
    "source-map-support": "^0.5.0",
    "typescript": "2.5.3"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "bunyan": "^1.8.12",
    "cors": "^2.8.4",
    "express": "^4.16.0",
